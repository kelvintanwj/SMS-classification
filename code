      "source": [
        "def clean_text(texts):\n",
        "        '''\n",
        "        Utility function to clean text by removing links, special characters\n",
        "        '''\n",
        "        return ' '.join(re.sub(\"(@[A-Za-z0-9]+)|([^0-9A-Za-z \\t])|(\\w+:\\/\\/\\S+)\", \" \", texts).split())"
      ],
      "execution_count": 0,
      "outputs": []
