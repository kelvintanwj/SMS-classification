        "tfidf = tfidf_vectorizer.fit_transform(clean_texts)\n",
        "tfidf_feature_names = tfidf_vectorizer.get_feature_names()\n",
        "pd.DataFrame(tfidf.toarray(), columns=tfidf_vectorizer.get_feature_names())"
