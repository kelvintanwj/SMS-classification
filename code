        "texts = sms_data['text']\n",
        "clean_texts = []\n",
        "for text in texts:\n",
        "    result = ''.join([i for i in text if not i.isdigit()])\n",
        "    result = clean_text(result)\n",
        "    clean_texts.append(result)"
      ],
      "execution_count": 0,
